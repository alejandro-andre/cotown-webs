<section class="cotownity">

  <div class="container-cotownity pure-g">

    <div class="pure-u-1 pure-u-lg-10-24">

      <div class="left">
        <div class="coral">
          <h2>{% literal texts, 'home-cotownity-titulo', L %}</h2>
          {% image
           constants.SERVER + "/wopi/files/Marketing/Media/" + images['cotownity'].id +"/Image/contents?access_token=" + token,
           "community", "community", "",
           [500], ""
          %}
          <div class="last-contain">
            <div class="insta-fb">
              <em class="icon-instagram"></em>
              <em class="icon-facebook"></em>
            </div>
          </div>
        </div>
      </div>

      <div class="feeling">
        <h2 class="title-feel">{% literal texts, 'home-cotownity-hashtag', L %}</h2>
      </div>

    </div>

    <div class="pure-u-1 pure-u-lg-14-24">
      <div class="right">
        <div class="widget">
          <div data-mc-src="18e30070-35e9-4ef7-bd1b-9ccb739eb24b#instagram"></div>
          <script
            src=https://cdn2.woxo.tech/a.js#64b91a6476bbbea0f1d8af04
            async data-usrc>
          </script>          
          </div>
      </div>
    </div>

  </div>

</section>

<section class="cotownity-mobile">

  <div class="coral">
    <h2>{% literal texts, 'home-cotownity-titulo', L %}</h2>
  </div>

  <div data-mc-src="18e30070-35e9-4ef7-bd1b-9ccb739eb24b#instagram"></div>
  <script
    src="https://cdn2.woxo.tech/a.js#64b91a6476bbbea0f1d8af04"
    async data-usrc>
  </script>

</section>

