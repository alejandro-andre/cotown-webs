<section class="partners">

  <div class="text-container">
    <h2 class="title twelve columns">
      {% literal texts, 'home-nuestros-partners', L %}
    </h2>
  </div>

  <section id="splide-partners" aria-label="Splide Basic HTML Example">
    <div class="splide__track">
      <ul class="splide__list">
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/241/400/400" alt="Imagen 9">
        </li>
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/242/400/400" alt="Imagen 10">
        </li>
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/243/400/400" alt="Imagen 11">
        </li>
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/248/400/400" alt="Imagen 12">
        </li>
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/249/400/400" alt="Imagen 13">
        </li>
        <li class="container splide__slide">
          <img class="img" src="https://picsum.photos/id/247/400/400" alt="Imagen 14">
        </li>
      </ul>
    </div>
  </section>

  <script>
  var splide = new Splide("#splide-partners", {
    type       : 'loop',
    fixedHeight: '20rem',
    gap        : '0.7rem',
    padding    : '5rem',
    perPage: 3,
    breakpoints: {
  		1024: { perPage: 2 },
  		640: { perPage: 1 },
    },
    perMove    : 1
  });
  splide.mount();
  </script>
  
  <div class="banner-partners">
    <div class="contain">
      <div class="img">1
      </div>
      <div class="img">2
      </div>
      <div class="img">3
      </div>
      <div class="img">4
      </div>
    </div>
    <div class="contain">
      <div class="img">5
      </div>
      <div class="img">6
      </div>
      <div class="img">7
      </div>
      <div class="img">8
      </div>
    </div>
  </div>

</section>
