<section class="pisos cotown">
    <div class="text-container">
      <h2 class="head2 title">{% literal texts, 'home-pisazos-elige', L %}</h2>
    </div>

  <div class="pure-g container">  
    <div class="pure-u-lg-1-2 pure-u-1 img-container">
      <a href="{{root}}{{langs[L].folder}}/{{langs[L].pages['locations'].url}}.html">
        
          <div class="img cotown">
            {% image
              constants.SERVER + "/wopi/files/Marketing/Media/" + images['apartamento-privado'].id +"/Image/contents?access_token=" + token,
              L, "apartamento-privado", "apartamento-privado", "apartamento-privado", "",
              [1280], ""
            %}
          </div>
          <div class="img-title cotown">
            <h6 class="head6">{% literal texts, 'home-pisazos-apartamento', L %}</h6>
            <h6 class="head6">{% literal texts, 'home-pisazos-privado', L %}</h6>
          </div>          
       
      </a>
    </div>

    <div class="pure-u-lg-1-2 pure-u-1 img-container">
      <a href="{{root}}{{langs[L].folder}}/{{langs[L].pages['locations'].url}}.html">
          <div class="img last cotown">
            {% image
              constants.SERVER + "/wopi/files/Marketing/Media/" + images['piso-compartido'].id +"/Image/contents?access_token=" + token,
              L, "piso-compartido", "piso-compartido", "piso-compartido", "",
              [1280], ""
            %}
          </div>
          <div class="img-title cotown second">
            <h6 class="head6">{% literal texts, 'home-pisazos-piso', L %}</h6>
            <h6 class="head6">{% literal texts, 'home-pisazos-compartido', L %}</h6>
          </div>          
      </a>
    </div>
  </div>

</section>
